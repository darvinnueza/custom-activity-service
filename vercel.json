{
    "version": 2,
    "builds": [{ "src": "api/index.js", "use": "@vercel/node" }],
    "routes": [
        { "src": "/docs/?", "dest": "/api/index.js" },
        { "src": "/swagger\\.yml", "dest": "/api/index.js" },
        { "src": "/health", "dest": "/api/index.js" },
        { "src": "/execute", "dest": "/api/index.js" },
        { "src": "/genesys/(.*)", "dest": "/api/index.js" }
    ]
}